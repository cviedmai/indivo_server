{% extends "reports/reports.xml" %}
{% load template_utils %}
{% block summary %}
  <Summary total_document_count="{{trc}}" limit="{{limit}}" offset="{{offset}}" order_by="{{order_by}}" />
{% endblock %}
{% block report %}
{% for scn in scns %}
<Report>
  <Meta>
    {{ scn.document_id|get_doc_obj }}
  </Meta>
  <Item>
    {% include "reports/simple_clinical_note.xml" %}
  </Item>
</Report>
{% endfor %}
{% endblock %}
